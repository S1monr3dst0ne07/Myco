    }
}

// Cleanup function for module environment
static void cleanup_module_env() {
    if (modules && modules_size > 0) {
        for (int i = 0; i < modules_size; i++) {
            if (modules[i].alias) {
                tracked_free(modules[i].alias, __FILE__, __LINE__, "cleanup_module_env");
