        // Find the last directory separator (cross-platform)
        char* last_sep = strrchr(path, '/');
        char* win_sep = strrchr(path, '\\');
        
        // Use the rightmost separator
        if (win_sep && (!last_sep || win_sep > last_sep)) {
            last_sep = win_sep;
