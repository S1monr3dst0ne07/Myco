        }
        
        // Extract filename (remove quotes)
        char filename[1024];
        if (is_string_literal(file_node->text)) {
            size_t len = strlen(file_node->text);
            if (len > 2) {
                strncpy(filename, file_node->text + 1, len - 2);
                filename[len - 2] = '\0';
            } else {
